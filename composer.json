{
    "name": "gladcodes/keygen",
    "description": "A fluent PHP random key generator.",
    "keywords": ["PHP", "random", "key", "generator", "keygen", "key generator"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Glad Chinda",
            "email": "gladxeqs@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0"
    },
    "suggest": {
        "ext-openssl": "It can help generator to get key with random bytes"
    },
    "autoload": {
        "psr-4": {
            "Keygen\\": "src/Keygen"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keygen\\Tests\\": "tests/Keygen"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.7 || ^6.5"
    }
}
